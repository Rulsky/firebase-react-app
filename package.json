{
  "name": "@rulsky/firebase-react-app",
  "version": "0.16.1",
  "description": "a tool to convert es6 code into compatible code with firebase cloud functions",
  "repository": {
    "type": "git",
    "url": "git@github.com:Rulsky/firebase-react-app.git"
  },
  "bin": {
    "fra": "./index.js"
  },
  "files": [
    "scripts",
    "helpers",
    "config",
    "devServer",
    "index.js",
    "jestTransformer.js"
  ],
  "engines": {
    "node": ">=6.14.0",
    "npm": ">=5.2.0"
  },
  "scripts": {
    "lint": "eslint 'config/**' 'devServer/**' 'helpers/**' 'scripts/**'",
    "test": "jest --coverage",
    "test:w": "jest --watch",
    "test:wc": "jest --no-cache --coverage --watch",
    "check": "npm run lint && npm run test",
    "cleandeps": "rm -rf package-lock.json yarn.lock node_modules/ && npm i && npm link"
  },
  "keywords": [],
  "author": "Ruslan Zaytsev <ruslan.zaytsev@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-classes": "7.0.0",
    "@babel/plugin-transform-destructuring": "7.0.0",
    "@babel/plugin-transform-react-constant-elements": "7.0.0",
    "@babel/plugin-transform-react-display-name": "7.0.0",
    "@babel/plugin-transform-regenerator": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.0",
    "babel-plugin-source-map-support": "^2.0.1",
    "babel-plugin-styled-components": "^1.6.3",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "commander": "^2.17.1",
    "cross-spawn": "^6.0.5",
    "css-loader": "^1.0.0",
    "express": "^4.16.3",
    "fs-extra": "^7.0.0",
    "http-proxy-middleware": "^0.19.0",
    "react-hot-loader": "^4.3.5",
    "source-map-support": "^0.5.9",
    "style-loader": "^0.23.0",
    "webpack": "^4.17.1",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.23.1"
  },
  "devDependencies": {
    "eslint": "^5.4.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-node": "^7.0.1",
    "husky": "^1.1.1",
    "jest": "^23.5.0",
    "jest-cli": "^23.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run check"
    }
  },
  "jest": {
    "watchPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/testSubject/"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/testSubject/"
    ],
    "collectCoverageFrom": [
      "scripts/**",
      "devServer/**",
      "helpers/**"
    ],
    "resetMocks": true,
    "resetModules": true,
    "restoreMocks": true,
    "testEnvironment": "node"
  }
}
